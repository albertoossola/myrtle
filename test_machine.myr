machine main { 
  state a { 
    os::timer(interval=200) 
      >> myrtle::literal(value=2) 
      >> os::print()
      >> myrtle::set_var(variable="@state");
  }

  state b {
    os::timer(interval=200) 
      >> myrtle::literal(value=0) 
      >> os::print() 
      >> myrtle::set_var(variable="@state");
  }
}